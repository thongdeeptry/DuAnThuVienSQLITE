<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SachMuonNhieuNhatFragment">

    <!-- TODO: Update blank fragment layout -->

    <EditText
        android:id="@+id/nhapkttop"
        android:layout_width="285dp"
        android:layout_height="54dp"
        android:layout_marginEnd="2dp"
        android:ems="10"
        android:hint="Nhập số top muốn check"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/kiemtratop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/kiemtratop"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginStart="2dp"
        android:text="Kiểm Tra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nhapkttop"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/lv"
        android:layout_width="416dp"
        android:layout_height="597dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nhapkttop" />

</androidx.constraintlayout.widget.ConstraintLayout>